{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "diagnostics.disable": [
        "undefined-field",
        "need-check-nil",
        "different-requires",
        "redundant-parameter",
        "param-type-mismatch"
    ],
    "diagnostics.globals": [
        "gma"
    ],
    "runtime.version": "Lua 5.3",
    "workspace.checkThirdParty": false,
    "workspace.ignoreDir": [
        "lib"
    ],
    "workspace.ignoreSubmodules": false,
    "workspace.library": [
        "lib"
    ]
}